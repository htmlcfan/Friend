@{
   Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=no" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title></title>
    @*添加Jquery EasyUI的样式*@
    <link href="~/Content/EasyUI/themes/default/easyui.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/index.css" rel="stylesheet" type="text/css" />
    @*添加Jquery，EasyUI和easyUI的语言包的JS文件*@
    <!--[if !IE]> -->
    <script src="~/Content/EasyUI/jquery-2.0.3.min.js"></script>
    <!-- <![endif]-->
    <!--[if IE]>
    <script src="~/Content/EasyUI/jquery-1.10.2.min.js"></script>
    <![endif]-->
    <script src="~/Content/EasyUI/jquery.easyui.min.js" type="text/javascript"></script>
    <script src="~/Content/EasyUI/locale/easyui-lang-zh_CN.js" type="text/javascript"></script>
    @*zTree树形菜单*@
    <link href="~/Content/zTreeStyle.css" rel="stylesheet" type="text/css" />
    <script src="~/Content/ZTree/jquery.ztree.core-3.5.min.js" type="text/javascript"></script>
    <script src="~/Content/ZTree/jquery.ztree.excheck-3.5.js" type="text/javascript"></script>
    @*日期格式的引用*@
    <script src="~/Content/EasyUI/datapattern2.js" type="text/javascript"></script>
    @*上传控件*@
    <script src="~/Content/Uploadify/jquery.uploadify.min.js" type="text/javascript"></script>
    <link href="~/Content/Uploadify/uploadify.css" rel="stylesheet" type="text/css" />

    <script src="~/ViewScript/Home.js"></script>
    <script>
       var bgAdmin = "@ViewBag.BgAdmin";
        var bgAdmin1="@ViewBag.BgAdmin1";
    </script>
    <!--[if lt IE 9]>
         <script src="~/Content/EasyUI/html5shiv.min.js"></script>
         <script src="~/Content/EasyUI/respond.min.js"></script>
     <![endif]-->
</head>
<body class="easyui-layout">
   <div id="OverTimeLogin" class="easyui-dialog" data-options="closed:true,modal:true">
      <iframe width="726px" scrolling="no" height="497px" frameborder="0" id="iOverTimeLogin"></iframe>
   </div>
   <div data-options="region:'north',border:false,split:true" style="height: 60px;">
      <div class="define-head">
         <div class="define-logo">
            <div id="LoginTopLine">管理系统后台</div>
            <div id="LoginBotoomLine"></div>
         </div>
         <div class="define-account">
          当前时间：<b id="date"></b>&nbsp;<a href="@ViewBag.BgAdmin/Home/LogOut" title="注销">
    <b>
        注销
    </b>
</a>&nbsp;
         </div>
      </div>
   </div>
   <div data-options="region:'west',split:true,title:'菜单列表'" style="width: 200px; height: 100%; padding-top: 2px; background-color: #fff; overflow: auto">
      <div id="RightTree" style="background-color: #fff;" class="ztree">
         <div class="panel-loading">加载中...</div>
      </div>
   </div>
   <div data-options="region:'south',border:false" style="height: 20px;">
      <div class="define-bottom">
      </div>
   </div>
   <div data-options="region:'center',border:false">
      <div id="mainTab" class="easyui-tabs" data-options="fit:true">
         <div title="我的桌面" data-options="closable:true" style="overflow: hidden; background: #fff">
             <iframe scrolling="auto" frameborder="0" src="@ViewBag.BgAdmin/Home/Center" style="width: 100%; height: 100%;"></iframe>
         </div>
      </div>
   </div>
   <div id="tab_menu" class="easyui-menu" style="width: 150px;">
      <div id="tab_menu-tabrefresh" data-options="iconCls:'icon-reload'">
         刷新
      </div>
      <div id="tab_menu-openFrame">
         在新的窗体打开
      </div>
      <div id="tab_menu-tabcloseall">
         关闭所有
      </div>
      <div id="tab_menu-tabcloseother">
         关闭其他标签页
      </div>
      <div class="menu-sep">
      </div>
      <div id="tab_menu-tabcloseright">
         关闭右边
      </div>
      <div id="tab_menu-tabcloseleft">
         关闭左边
      </div>
      <div id="tab_menu-tabclose" data-options="iconCls:'icon-remove'">
         关闭
      </div>
      <div id="menu" class="easyui-menu" style="width: 150px;">
      </div>
   </div>
</body>
</html>

